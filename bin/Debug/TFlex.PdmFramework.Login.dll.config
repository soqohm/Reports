<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="useNewClient" value="True"/>

    <!-- Поддерживаемые алгоритмы сжатия / Available compression algorithms: None, GZip, LZ4, LZ4High, ZSTD -->
    <add key="CompressionAlgorithm" value="LZ4"/>

    <!-- Поддерживаемые алгоритмы сериализации / Available data serialization algorithms: Default, Protobuf, ZeroFormatter -->
    <add key="DataSerializerAlgorithm" value="ZeroFormatter"/>

    <!-- Время ожидания запросов к файловому серверу в минутах / File server requests timeout in minutes -->
    <!--<add key="FileServerTimeout" value="10" />-->
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>
